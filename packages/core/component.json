{
  "componentId": {
    "scope": "ngx-translate.packages",
    "name": "core"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageManagerPublishArgs": [
        "--access public"
      ],
      "packageJson": {
        "name": "@ngx-translate/core",
        "private": false,
        "engines": {
          "node": "^16.13.0 || >=18.10.0"
        }
      }
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "@angular/router": "-"
        },
        "peerDependencies": {
          "@angular/core": ">=16.0.0",
          "@angular/common": ">=16.0.0"
        },
        "devDependencies": {
          "@angular/router": ">=16.0.0"
        }
      }
    },
    "teambit.angular/versions/angular-v16@1.0.2": {},
    "teambit.envs/envs": {
      "env": "teambit.angular/versions/angular-v16"
    }
  }
}
